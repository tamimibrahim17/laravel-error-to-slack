{
    "name": "tamimibrahim17/laravel-error-to-slack",
    "description": "Send a message to a Slack channel when error occurs in your Laravel Application",
    "require": {
        "php": ">=7.0",
        "illuminate/support": "^5.5",
        "monolog/monolog": "~1.12"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Gaurav Makhecha",
            "email": "info@freshbits.in"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Freshbitsweb\\SlackErrorNotifier\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Freshbitsweb\\SlackErrorNotifier\\SlackErrorNotifierServiceProvider"
            ]
        }
    }
}
